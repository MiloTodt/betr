egrep -o '("name":(.*)",)|(<b>[+|-]([1-9][0-9][0-9])<\/b>)|(<b>[+|-]([1-9][0-9][0-9][0-9])<\/b>)|rel="[0-9]{1,4}-[0-9]{1,2}-[0-9]{1,2}' betHtml.txt |sed 's/\"UDF\",//' | sed 's/\"//' | sed 's/\",//'|sed 's/<b>//' | sed 's/<\/b>//' | sed 's/name\": //' |sed 's/\"//' | sed '/^ *$/d' |sed 's/rel=//' > betInfo.txt